# Copy this file to `.env` at the repository root and fill in your secrets locally.
# Do NOT commit `.env` to git.

# OpenAI / ChatGPT
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDINGS_MODEL=text-embedding-3-small

# Supabase (optional)
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Service role is optional and should NOT be used in the browser; for backend use only.
SUPABASE_SERVICE_ROLE_KEY=

# Supabase MCP (optional)
# If you run the Supabase MCP server, provide either an access token (account scope)
# or project-level refs/keys depending on your setup.
SUPABASE_ACCESS_TOKEN=
SUPABASE_PROJECT_REF=
SUPABASE_MCP_SERVER_URL=

# App settings (safe defaults)
EVIDENCE_STRICTNESS=balanced  # strict|balanced|lenient
TOKEN_BUDGET_PER_JOB=50000

